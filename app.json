{
  "expo": {
    "name": "TimeSaude - Medication Reminder",
    "slug": "timesaude",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timesaude.medicationreminder"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6366F1"
      },
      "package": "com.timesaude.medicationreminder",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-notifications",
      "expo-sqlite",
      "expo-location",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar to schedule medication reminders."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}